# Cube with all 6 faces
v -1.0 -1.0  1.0  # front bottom left  (1)
v  1.0 -1.0  1.0  # front bottom right (2)
v  1.0  1.0  1.0  # front top right    (3)
v -1.0  1.0  1.0  # front top left     (4)
v -1.0 -1.0 -1.0  # back bottom left   (5)
v  1.0 -1.0 -1.0  # back bottom right  (6)
v  1.0  1.0 -1.0  # back top right     (7)
v -1.0  1.0 -1.0  # back top left      (8)

# Texture coordinates for a cube (each face uses the same UV mapping)
vt 0.0 0.0
vt 1.0 0.0
vt 1.0 1.0
vt 0.0 1.0

# Normals
vn  0.0  0.0  1.0  # front
vn  0.0  0.0 -1.0  # back
vn  0.0  1.0  0.0  # top
vn  0.0 -1.0  0.0  # bottom
vn  1.0  0.0  0.0  # right
vn -1.0  0.0  0.0  # left

# Faces (12 triangles total - 2 per face)
# Front face
f 1/1/1 2/2/1 3/3/1
f 3/3/1 4/4/1 1/1/1

# Back face
f 5/1/2 8/2/2 7/3/2
f 7/3/2 6/4/2 5/1/2

# Top face
f 4/1/3 3/2/3 7/3/3
f 7/3/3 8/4/3 4/1/3

# Bottom face
f 1/1/4 5/2/4 6/3/4
f 6/3/4 2/4/4 1/1/4

# Right face
f 2/1/5 6/2/5 7/3/5
f 7/3/5 3/4/5 2/1/5

# Left face
f 5/1/6 1/2/6 4/3/6
f 4/3/6 8/4/6 5/1/6